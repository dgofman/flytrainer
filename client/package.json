{
  "name": "flytrainerapp",
  "version": "1.2.0",
  "main": "electron_main.js",
  "description": "Flight School Management System",
  "author": {
    "name": "David Gofman",
    "email": "dgofmancfi@gmail.com"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --proxy-config src/proxy.conf.js",
    "build": "ng build",
    "build:prod": "npm run build -- -c production",
    "cleancss": "grunt purifycss",
    "build:electron": "npm run build -- -c electron --base-href ./index.html",
    "electron:local": "npm run build:electron && npx electron .",
    "electron:build": "npm run build:electron && electron-builder build",
    "electron": "electron .",
    "lint": "ng lint"
  },
  "build": {
    "productName": "FlyTrainer",
    "artifactName": "FlyTrainer.exe",
    "appId": "com.flytrainer.fsms",
    "win": {
      "icon": "src/assets/favicon.png",
      "target": [
        "nsis"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "../electorn-release"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.1",
    "@angular/cdk": "^11.0.1",
    "@angular/common": "~11.0.1",
    "@angular/compiler": "~11.0.1",
    "@angular/core": "~11.0.1",
    "@angular/forms": "~11.0.1",
    "@angular/platform-browser": "~11.0.1",
    "@angular/platform-browser-dynamic": "~11.0.1",
    "@angular/router": "~11.0.1",
    "@fortawesome/angular-fontawesome": "^0.8.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "primeicons": "^4.1.0",
    "primeng": "^9.1.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "xlsx": "^0.16.9",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.1",
    "@types/node": "^14.14.10",
    "codelyzer": "^6.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1",
    "glob": "^7.1.6",
    "grunt": "^1.3.0",
    "grunt-purifycss": "^0.1.2"
  }
}
